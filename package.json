{
	"name": "weeb_friendly",
	"productName": "Weeb Friendly",
	"version": "0.1.1",
	"private": true,
	"author": {
		"name": "Guydht",
		"url": "https://github.com/guydht",
		"email": "guydht1@gmail.com"
	},
	"icon": "./icons/",
	"description": "App for managing and downloading Animes from myanimelist/torrents respectively.",
	"eslintConfig": {
		"extends": "react-app"
	},
	"build": {
		"linux": {
			"asar": true,
			"target": [
				"AppImage",
				"deb"
			],
			"category": "Video",
			"icon": "./icons/"
		},
		"win": {
			"icon": "./icons/icon.ico",
			"target": [
				"portable",
				"nsis"
			]
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"electron": "^8.2.5",
		"electron-builder": "^22.6.0"
	},
	"dependencies": {
		"@types/rc-slider": "^8.6.5",
		"@types/react": "^16.9.34",
		"@types/react-medium-image-zoom": "^3.0.0",
		"@types/react-router-dom": "^5.1.5",
		"@types/webtorrent": "^0.107.2",
		"bootstrap": "^4.4.1",
		"electron-is-dev": "^1.2.0",
		"electron-json-config": "^1.5.3",
		"electron-window-state": "^5.0.3",
		"eslint": "^6.8.0",
		"jikan-node": "^1.2.0",
		"jikants": "^1.2.13",
		"matroska-subtitles": "^2.0.3",
		"moment": "^2.25.3",
		"nyaapi": "^2.1.3",
		"qwant-api": "^0.5.0",
		"rc-slider": "^9.2.4",
		"react": "^16.13.1",
		"react-bootstrap": "^1.0.1",
		"react-countdown-circle-timer": "^1.1.1",
		"react-dom": "^16.13.1",
		"react-lazy-load-image-component": "^1.4.3",
		"react-medium-image-zoom": "^3.1.2",
		"react-router-dom": "^5.1.2",
		"react-scripts": "^5.0.1",
		"react-sortablejs": "^1.5.1",
		"sortablejs": "^1.10.2",
		"typescript": "^3.8.3",
		"webtorrent": "^0.108.1"
	},
	"main": "public/electron.js",
	"scripts": {
		"start": "electron . & react-scripts start",
		"build": "react-scripts build && electron-builder .",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"electron": "electron .",
		"react": "react-scripts start",
		"build-electron": "electron-builder ."
	},
	"homepage": "./"
}
